{
  "name": "ts-code",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "dependencies": {
    "express": "^4.17.1",
    "ejs": "^3.1.6"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.4",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "clean": "rimraf build/",
    "copy-public-folder": "fse copy src/public/ build/public",
    "copy-routes-folder": "fse copy src/routes/ build/routes",
    "copy-views-folder": "fse copy src/views/ build/views",
    "copy-artifacts": "npm run copy-public-folder && npm run copy-routes-folder && npm run copy-views-folder",
    "build": "npm run clean && tsc && npm run copy-artifacts",
    "start": "npm run build && node build/app.js",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
