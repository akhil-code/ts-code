  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />

    <!-- css styling -->
    <style media="screen">
      /* Title */
      .title-icon {
        padding: 10px 20px;
      }

      /* Navbar */
      .navbar {
        list-style-type: none;
        padding: 0;
        margin: 0;
        overflow: hidden;
        background-color: #ffffff;
        margin-left: auto;
        margin-right: auto;
        width: fit-content;
      }

      .navbar li {
        float: left;
      }

      /* Search box */
      .search-box {
        box-sizing: border-box;
        padding: 10px 0;
        width: 300px;
      }

      /* Style the search field */
      .search-box input[type="text"] {
        padding: 4px;
        font-size: 12px;
        border: 1px solid grey;
        background: #f1f1f1;
        border-style: none;
      }

      /* Button holder */
      #button-holder {
        list-style-type: none;
        padding: 0;
        margin: 0;
        overflow: hidden;
      }

      #button-holder li {
        float: left;
      }

      #button-holder li a {
        display: block;
        color: black;
        text-align: center;
        padding: 12px 8px;
        text-decoration: none;
        font-size: 15px;
      }
    </style>
  </head>
