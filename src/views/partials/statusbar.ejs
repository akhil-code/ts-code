<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <style>
      body {
        background-color: yellow;
        margin: 0;
        padding: 0;
      }

      .profile-photo-container {
        list-style-type: none;
        margin: 0;
        padding: 16px 0px;
        overflow-x: scroll;
        border: 1px solid #dbdbdb;
        width: 600px;
        margin-left: auto;
        margin-right: auto;
        display: flex;
      }

      .profile-photo-container li {
          float: left;
          padding: 0px 10px;
      }

      .profile-photo-container li img {
        border-radius: 50%;
        height: 56px;
        width: 56px;
        margin-left: auto;
        margin-right: auto;
        display: block;
      }

      .profile-photo-container li div {
        text-align: center;
      }
    </style>
  </head>
  <body>
    <div class="statubar-container">
      <ul class="profile-photo-container">
        <% for(var i=0;i<10;i++) { %>
        <li>
          <img
            src="https://images.unsplash.com/photo-1532074205216-d0e1f4b87368?ixid=MnwxMjA3fDB8MHxzZWFyY2h8MXx8d29tYW4lMjBwcm9maWxlfGVufDB8fDB8fA%3D%3D&ixlib=rb-1.2.1&w=1000&q=80"
          />
          <div>that.me.akhil</div>
        </li>
        <% } %>
      </ul>
    </div>
  </body>
</html>
